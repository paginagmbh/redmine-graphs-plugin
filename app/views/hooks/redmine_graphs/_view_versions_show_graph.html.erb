<% if @version.issues_count > 0 && !action_name.nil? && !version.nil? && User.current.allowed_to?(:view_graphs, @project) %>
    <%= tag('embed', width: '100%', height: 300, type: 'image/svg+xml', src: url_for(controller: 'graphs', action: action_name, id: version)) %>
<% else %>
    <%= render partial: 'issues/list_simple', locals: {issues: @issues} %>
<% end %>
