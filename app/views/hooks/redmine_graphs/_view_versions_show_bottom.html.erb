  <% if @spent_hours != 0 || @estimated_hours != 0 %>
    <% if Setting.plugin_redmine_graphs[:total_vs_closed_issues_enabled] ? true : false %>
      <fieldset id='target_version_graph'><legend><%= l(:label_graphs_total_vs_closed_issues) %></legend>
        <%= tag("embed", :width => "100%", :height => 300, :type => "image/svg+xml", :src => url_for(:controller => 'graphs', :action => 'target_version_graph', :id => version))%>
      </fieldset>
    <% end %>
    <% if Setting.plugin_redmine_graphs[:burndown_enabled] ? true : false %>
      <fieldset id='target_version_graph'><legend><%= l(:label_graphs_burndown) %></legend>
        <%= tag("embed", :width => "100%", :height => 300, :type => "image/svg+xml", :src => url_for(:controller => 'graphs', :action => 'burndown_graph', :id => version))%>
      </fieldset>
    <% end %>
  <% end %>
